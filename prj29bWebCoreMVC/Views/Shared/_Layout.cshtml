				<li class="nav-item dropdown">
					<a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#">Customer</a>
					<ul class="dropdown-menu">
						<li>
							<a class="nav-link text-dark" asp-area="" asp-controller="Customer" asp-action="Index">CustomerIndex30</a>
						</li>
						<li>
							<a class="nav-link text-dark" asp-area="" asp-controller="Customer" asp-action="Phone">CustomerPhone31</a>
						</li>
						<li>
							<a class="nav-link text-dark" asp-area="" asp-controller="Customer" asp-action="Country">CustomerCountry32</a>
						</li>
					</ul>
				</li>


				<li class="nav-item dropdown">
					<a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#">Student</a>
					<ul class="dropdown-menu">
						<li>
							<a class="nav-link text-dark" asp-area="" asp-controller="Student" asp-action="Index">StudentIndex40</a>
